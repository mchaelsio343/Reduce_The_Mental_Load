<div class="modal-body row">
	<div class="col-md-6">
        <h2>Completed Appointments</h2>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Date</th>
                </tr>
            </thead>
            <tbody>
                {{#each APPOINTMENT}}
                <tr>
                    <td>{{Name}}</td>
                    <td>{{Date}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>

    <div class="col-md-6">
        <h2>Completed Tasks</h2>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Date</th>
                </tr>
            </thead>
            <tbody>
                {{#each TODO}}
                <tr>
                    <td>{{Name}}</td>
                    <td>{{Date}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
<div class="modal-body row">
	<div class="col-md-6">
		<a href="/" class="btn btn-primary">Go Home</a>
	</div>
</div>